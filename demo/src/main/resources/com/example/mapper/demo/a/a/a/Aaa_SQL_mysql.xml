<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.a.a.a.service.impl.AaaMapper">

	<select id="selectAaaList"
		parameterType="com.example.demo.a.a.a.service.AaaVo"
		resultType="com.example.demo.a.a.a.service.AaaVo">
<![CDATA[
SELECT /* AaaMapper.selectAaaList */
       CODE_ID
     , CODE
     , CODE_NM
     , CODE_DC
  FROM LETTCCMMNDETAILCODE
 WHERE USE_AT = 'Y'
   AND CODE_ID = #{codeId}
]]>
	</select>

</mapper>